    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "buildFlags": "-race",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "FARGO_LOG_LEVEL":"DEBUG",
                "REGISTRATOR_LOG_LEVEL":"DEBUG",
                "SERVICE_EUREKA_DATACENTERINFO_AUTO_POPULATE":"false",
                "DOCKER_HOST": "unix:///var/run/docker.sock"
            },
            "args": ["-ttl", "30", "-ttl-refresh", "15", "-ip-lookup-source", "http://ipify.app.thorhudl.com/", "-require-label", "eureka://localhost:8080/eureka/v2"]
        }
    ]
}